<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar"></style>

    <!-- Application theme. -->
    <style name="DemoAppTheme" parent="AppBaseTheme"></style>

    <style name="container_demo_standard">
        <item name="android:background">@drawable/grey_frame_on_white</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
    </style>

</resources>
